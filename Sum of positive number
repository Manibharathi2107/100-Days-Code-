import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a[]=new int[n];
        int sum=0;
        for(int i=0;i<n;i++){
            a[i]=sc.nextInt();
            if(a[i]>0)
                sum=sum+a[i];
        }
        System.out.printf("The sum of positive number is %d :",sum);
    }
}
