import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        if(n==0){
            System.out.println("Zero");
        }
        else if(n<0){
            System.out.println("Negative");
        }
        else{
            System.out.println("Positive");
        }
    }
}
